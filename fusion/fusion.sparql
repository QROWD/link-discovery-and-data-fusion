# Add Representand
INSERT {
  ?cluster
    qo:representedBy ?rep ;
    .
  ?rep
    a qo:Representand ;
    .
} WHERE {
  ?cluster
    a qo:SameAsCluster ;
    .
  BIND(iri(concat(str(qr:), struuid())) AS ?rep)
}

# Add best ranked property to Representand
INSERT {
  ?rep
    ?p ?o ;
    qo:propertySource ?memberProperty ;
    .
} WHERE {
  {
    SELECT ?rep ?p (max(?rank) AS ?maxRank) {
      ?cluster
        qo:representedBy ?rep ;
        qo:memberProperty ?memberProperty ;
        .
      ?memberProperty
        owl:annotatedSource/prov:wasDerivedFrom/db:rank ?rank ;
        owl:annotatedProperty ?p ;
        .
    } GROUP BY ?rep ?p
  }
  ?cluster
    qo:representedBy ?rep ;
    qo:memberProperty ?memberProperty ;
    .
  ?memberProperty
    owl:annotatedSource/prov:wasDerivedFrom/db:rank ?rank ;
    owl:annotatedProperty ?p ;
    owl:annotatedTarget ?o ;
    .
  FILTER(?rank = ?maxRank)
  FILTER(?p != prov:wasDerivedFrom)
}

