# Update capacity
DELETE {
  ?s lgdo:capacity ?p ;
} WHERE {
  ?s a qo:Representand ;
    lgdo:capacity ?p ;
}

INSERT {
  ?rep lgdo:capacity ?capacity ;
} WHERE {
  SELECT ?rep (sum(xsd:integer(?cap)) AS ?capacity) {
    ?rep
      a qo:Representand ;
      qo:propertySource ?memberProperty ;
      .
    ?memberProperty
      owl:annotatedProperty lgdo:capacity ;
      owl:annotatedTarget ?cap ;
  } GROUP BY ?rep
}

CONSTRUCT WHERE { ?s ?p ?o }
